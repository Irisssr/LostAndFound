<view><view data-event-opts="{{[['tap',[['isMask',['$event']]]]]}}" hidden="{{!(isNum)}}" class="mask" bindtap="__e"></view><view class="user"><view class="userInfo"><view class="user_avatar"><image src="{{userInfo.userHead}}" class="_img"></image></view><view hidden="{{!(userInfo.userName)}}" class="user_name">{{userInfo.userName}}</view><block wx:if="{{userInfo.stuNum}}"><view data-event-opts="{{[['tap',[['editNum',['$event']]]]]}}" class="user_stunum" bindtap="__e"><label class="iconfont _span"></label>{{'学号:'+userInfo.stuNum+''}}</view></block><block wx:else><view data-event-opts="{{[['tap',[['editNum',['$event']]]]]}}" class="user_stunum" bindtap="__e">[请绑定学号]</view></block></view></view><view hidden="{{!(isNum)}}" class="bindingStu"><input type="number" placeholder="请填写学号" data-event-opts="{{[['input',[['__set_model',['','card_num','$event',[]]]]]]}}" value="{{card_num}}" bindinput="__e"/><button type="default" data-event-opts="{{[['tap',[['bindStuNum',['$event']]]]]}}" bindtap="__e">确认更新</button></view><view class="userList" animation="{{animationData}}"><button data-event-opts="{{[['tap',[['myPublish',['$event']]]]]}}" class="listItem" bindtap="__e"><label class="iconfont _span"></label><label class="_span">我的发布</label></button><button data-event-opts="{{[['tap',[['toAbout',['$event']]]]]}}" class="listItem" bindtap="__e"><label class="iconfont _span"></label><label class="_span">关于我们</label></button><button data-event-opts="{{[['tap',[['toQun',['$event']]]]]}}" class="listItem" bindtap="__e"><label class="iconfont _span"></label><label class="_span">信息群号</label></button><button class="listItem" open-type="share"><label class="iconfont _span"></label><label class="_span">分享该小程序</label></button><button data-event-opts="{{[['tap',[['toDesk',['$event']]]]]}}" class="listItem" bindtap="__e"><label class="iconfont _span"></label><label class="_span">创建桌面图标</label></button></view></view>