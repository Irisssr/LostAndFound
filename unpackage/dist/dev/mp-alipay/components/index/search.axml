<view class="search"><form class="search-form"><input type="text" placeholder="输入物品名" confirm-type="search" data-event-opts="{{[['focus',[['showdel',['$event']]]],['blur',[['hidedel',['$event']]]],['confirm',[['addSearch',['$event']]]],['input',[['__set_model',['','value','$event',[]]]]]]}}" value="{{value}}" onFocus="__e" onBlur="__e" onConfirm="__e" onInput="__e"/><view data-event-opts="{{[['tap',[['delkey',['$event']]]]]}}" hidden="{{!(del)}}" class="iconfont icon_delete _i" onTap="__e"></view></form><view data-event-opts="{{[['tap',[['searchKey',['$event']]]]]}}" class="iconfont icon_search _i" onTap="__e"></view></view>