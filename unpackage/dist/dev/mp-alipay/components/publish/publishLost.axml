<view class="pubbox data-v-1e7d6cad"><view class="pubMessage data-v-1e7d6cad"><view class="msg_title data-v-1e7d6cad"><input type="text" placeholder="标题(如:18计算机张三的学生卡,字数不超过20)" maxlength="20" data-event-opts="{{[['input',[['__set_model',['','title','$event',[]]]]]]}}" value="{{title}}" onInput="__e" class="data-v-1e7d6cad"/></view><view class="msg_info data-v-1e7d6cad"><textarea value="{{text}}" placeholder="详情(可选,字数不超过50)" maxlength="50" data-event-opts="{{[['input',[['__set_model',['','text','$event',[]]]]]]}}" onInput="__e" class="data-v-1e7d6cad"></textarea><view class="msg_pictures _ul data-v-1e7d6cad"><view data-event-opts="{{[['tap',[['getPicture',['$event']]]]]}}" class="pictures _li data-v-1e7d6cad" onTap="__e"><view class="iconfont _i data-v-1e7d6cad"></view></view><block a:for="{{imageList}}" a:for-item="item" a:for-index="index" a:key="index"><view class="_li data-v-1e7d6cad"><image src="{{item}}" mode="aspectFill" data-event-opts="{{[['tap',[['getPreview',[index]]]]]}}" onTap="__e" class="data-v-1e7d6cad"></image><view data-event-opts="{{[['tap',[['deleteImg',[index]]]]]}}" class="deleteImg iconfont data-v-1e7d6cad" onTap="__e"></view></view></block></view></view></view><pub-classify onSendClass="__e" vue-id="1" data-event-opts="{{[['^sendClass',[['getClass']]]]}}" class="data-v-1e7d6cad" onVueInit="__l"></pub-classify><relation onSendRelation="__e" vue-id="2" data-event-opts="{{[['^sendRelation',[['getRelation']]]]}}" class="data-v-1e7d6cad" onVueInit="__l"></relation><view class="cor_input data-v-1e7d6cad"><block a:if="{{relationType=='place'}}"><input type="text" placeholder="填写指定地点" data-event-opts="{{[['input',[['__set_model',['','relation','$event',[]]]]]]}}" value="{{relation}}" onInput="__e" class="data-v-1e7d6cad"/></block><block a:if="{{relationType=='qq'}}"><input type="number" placeholder="填写qq" data-event-opts="{{[['input',[['__set_model',['','relation','$event',[]]]]]]}}" value="{{relation}}" onInput="__e" class="data-v-1e7d6cad"/></block><block a:if="{{relationType=='tel'}}"><input type="number" placeholder="填写联系电话" maxlength="11" data-event-opts="{{[['input',[['__set_model',['','relation','$event',[]]]]]]}}" value="{{relation}}" onInput="__e" class="data-v-1e7d6cad"/></block></view><button class="cor_btn data-v-1e7d6cad" type="default" data-event-opts="{{[['tap',[['lostSubmit',['$event']]]]]}}" onTap="__e">发布</button></view>