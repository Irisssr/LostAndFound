<view><view data-event-opts="{{[['tap',[['isMask',['$event']]]]]}}" hidden="{{!(isNum)}}" class="mask" onTap="__e"></view><view class="user"><view class="userInfo"><view class="user_avatar"><block a:if="{{userInfo.userHead}}"><image src="{{userInfo.userHead}}" class="_img"></image></block><block a:else><image src="/static/logo.png" class="_img"></image></block></view><view hidden="{{!(userInfo.userName)}}" class="user_name">{{"昵称:"+userInfo.userName}}</view><view data-event-opts="{{[['tap',[['editNum',['$event']]]]]}}" hidden="{{!(userInfo.stuNum)}}" class="user_stunum" onTap="__e"><label class="iconfont _span"></label>{{'学号:'+userInfo.stuNum}}</view></view></view><view hidden="{{!(isNum)}}" class="bindingStu"><input type="number" placeholder="请填写学号" data-event-opts="{{[['input',[['__set_model',['','card_num','$event',[]]]]]]}}" value="{{card_num}}" onInput="__e"/><button type="default" data-event-opts="{{[['tap',[['bindStuNum',['$event']]]]]}}" onTap="__e">确认绑定</button></view><view class="userList"><view data-event-opts="{{[['tap',[['myPublish',['$event']]]]]}}" class="listItem" onTap="__e"><label class="iconfont _span"></label><label class="_span">我的发布</label></view><view data-event-opts="{{[['tap',[['toAbout',['$event']]]]]}}" class="listItem" onTap="__e"><label class="iconfont _span"></label><label class="_span">关于</label></view></view></view>