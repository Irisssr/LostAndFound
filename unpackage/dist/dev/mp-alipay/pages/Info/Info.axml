<view class="info data-v-69628721"><block a:if="{{mask===true}}"><view class="data-v-69628721"><view data-event-opts="{{[['tap',[['isMask',['$event']]]]]}}" class="mask data-v-69628721" onTap="__e"></view><canvas canvas-id="myCanvas" class="data-v-69628721"></canvas><view class="bottom data-v-69628721"><view class="savePhone data-v-69628721"><button type="default" data-event-opts="{{[['tap',[['savePhone',['$event']]]]]}}" onTap="__e" class="data-v-69628721">保存图片</button></view><view class="share data-v-69628721"><button type="default" open-type="share" class="data-v-69628721">分享</button></view></view></view></block><block a:if="{{mask===false}}"><view class="data-v-69628721"><view data-event-opts="{{[['tap',[['setPoster',['$event']]]]]}}" class="spread data-v-69628721" onTap="__e">扩散</view></view></block><view data-event-opts="{{[['tap',[['toThePublish',['$0'],['dataInfo.openid']]]]]}}" class="avatar data-v-69628721" onTap="__e"><view class="avatar_left data-v-69628721"><image src="{{dataInfo.user.userHead}}" mode="aspectFit" class="data-v-69628721"></image></view><view class="avatar_right data-v-69628721"><view class="avatar_name data-v-69628721">{{dataInfo.user.userName}}</view><view class="avatar_time data-v-69628721">{{dataInfo.time}}</view></view></view><view class="content data-v-69628721"><view class="title data-v-69628721">{{"标题: "+dataInfo.goodTitle}}</view><view class="text data-v-69628721">{{"详情: \n\t\t "+dataInfo.goodTexts}}</view><view class="relation data-v-69628721">{{"联系: "+relate+''}}<block a:if="{{reltype==='qq'||reltype==='tel'}}"><view data-event-opts="{{[['tap',[['cuttext',['$event']]]]]}}" class="cutbtn data-v-69628721" onTap="__e"><block a:if="{{reltype==='qq'}}"><label class="iconfont _span data-v-69628721"></label></block><block a:if="{{reltype==='tel'}}"><label class="iconfont _span data-v-69628721"></label></block><label class="_span data-v-69628721">复制</label></view></block></view></view><view class="imgList data-v-69628721"><image src=